{
    "name": "my-react-calendar",
    "version": "1.2.2",
    "private": false,
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yaowang908/my-react-calendar.git"
    },
    "author": "Yao Wang",
    "keywords": [
        "react",
        "calendar"
    ],
    "dependencies": {
        "@babel/runtime": "^7.14.6",
        "@craco/craco": "^6.1.2",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.33",
        "nanoid": "^3.1.23",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-render-html": "^0.6.0",
        "react-scripts": "4.0.3",
        "react-timezone-select": "^1.0.4",
        "recoil": "^0.3.1",
        "web-vitals": "^1.0.1"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build && yarn gulp",
        "test": "craco test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public -o ./docs",
        "prepublish": "rm -rf dist && mkdir dist && NODE_ENV=production babel ./src/ -d dist --copy-files && del dist/**/*.test.js dist/**/*.stories.js dist/stories",
        "prepublish-windows": "rm -rf dist && mkdir dist && cross-env NODE_ENV=production babel ./src/ -d dist --copy-files"
    },
    "babel": {
        "presets": [
            [
                "react-app",
                {
                    "absoluteRuntime": false
                }
            ],
            "@babel/preset-react"
        ],
        "plugins": [
            [
                "module-resolver",
                {
                    "root": [
                        "."
                    ],
                    "alias": {
                        "@root": "./src"
                    }
                }
            ]
        ],
        "ignore": [
            "src/stories/*"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@storybook/addon-actions": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/node-logger": "^6.2.9",
        "@storybook/preset-create-react-app": "^3.1.7",
        "@storybook/react": "^6.2.9",
        "autoprefixer": "^9",
        "babel-plugin-module-resolver": "^4.1.0",
        "craco-alias": "^3.0.1",
        "cross-env": "^7.0.3",
        "del-cli": "^4.0.0",
        "gulp": "^4.0.2",
        "gulp-inline-source": "^4.0.0",
        "gulp-replace": "^1.1.3",
        "postcss": "^7",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat"
    },
    "resolutions": {
        "babel-loader": "8.1.0"
    },
    "license": "MIT"
}
